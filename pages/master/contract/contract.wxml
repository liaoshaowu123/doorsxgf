<view class="item">
  <view>
      <view class='xx-title'>服务评分:</view>
      <image src='{{Index0>=index?"/images/xx-s.png":"/images/xx-n.png"}}' class='xx-img'
        wx:for='12345'
        data-index='{{index}}'
        catchtap='onSelect1'
      ></image>
      <text style="flex:1;text-align:center;font-size:28rpx;">{{strs[Index0]}}</text>
  </view>
  <view>
      <view class='xx-title'>团长服务评价:</view>
      <image src='{{Index1>=index?"/images/xx-s.png":"/images/xx-n.png"}}' class='xx-img'
        wx:for='12345'
        data-index='{{index}}'
        catchtap='onSelect2'
      ></image>
      <text style="flex:1;text-align:center;font-size:28rpx;">{{strs[Index1]}}</text>
  </view>
  <view>
      <view class='xx-title'>安装评价:</view>
      <image src='{{Index2>=index?"/images/xx-s.png":"/images/xx-n.png"}}' class='xx-img'
        wx:for='12345'
        data-index='{{index}}'
        catchtap='onSelect3'
      ></image>
      <text style="flex:1;text-align:center;font-size:28rpx;">{{strs[Index2]}}</text>
  </view>
</view>
<view class="button" catchtap="submitPJ">提交</view>
