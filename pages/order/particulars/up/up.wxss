/* pages/order/particulars/up/up.wxss */
page{
  background: #F9F9F9;
}
.img{
  width: 95%;
  height: 500rpx;
  margin: 0 auto;
  border-radius: 5px;
  background: #fff;
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.img>image{
  width: 140rpx;
  height: 140rpx;
  padding: 10rpx;
}
.button{
  margin-top: 60rpx;
  width: 100%;
  text-align: center;
}
.button>view{
  display: inline-block;
  padding: 15rpx 80rpx;
  font-size: 16px;
  color:#fff;
  background: #E70012;
  border-radius: 30px;
}